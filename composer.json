    "keywords": [
        "wordpress",
        "markdown",
        "readme",
        "converter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin J. Balter"
        },
        {
            "name": "Christian Archer",
            "email": "sunchaser@sunchaser.info"
        }
    ],
    "require": {
